  },
  "homepage": "https://github.com/slessans/joiful-query-params#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "5.0.1",
    "express": "4.13.4",
    "istanbul": "0.4.2",
    "joi": "8.0.0",
    "mocha": "2.4.5",
    "sinon": "1.17.3",
    "supertest": "1.2.0"
  },
  "peerDependencies": {
    "joi": "8.0.x"
